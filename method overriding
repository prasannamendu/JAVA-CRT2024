class Father
{
    void surName()
    {
        System.out.print("k");
    }
    void designation()
    {
        System.out.println("Clerk");
    }
}
class Child extends Father
{
    void getName()
    {
         System.out.println("prasanna");
    }
    void designation()
        {
        System.out.println("Java programmer");
    }
}
class Test
{
    public static void main(String args[])
    {
        Child c1=new Child();
        c1.surName();
        c1.getName();
        c1.designation();
    }
}
